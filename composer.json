{
    "name": "n30/laravel-omnisend-api",
    "description": "A laravel library for interacting with the Omnisend API v3",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Neo Nosrati",
            "email": "github@columbussoft.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "N30\\Omnisend\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "N30\\Omnisend\\Laravel\\ServiceProvider"
            ]
        }
    },
     "config": {
        "sort-packages": true
    }
}
